import argparse

def main():
    parser = argparse.ArgumentParser("RunXom")
    parser.add_argument("runs", nargs='+',help="Run number to process")
    parser.add_argument("--test", type=str,help="Run number to process")
    args = parser.parse_args()    
    for r in args.runs:
        
        print(r)

if __name__ == "__main__":
    main()

